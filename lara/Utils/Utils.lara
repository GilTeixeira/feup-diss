function intersectionSets(setA, setB){
    var _intersection = new Set();
    for (var elem of setB) {
        if (setA.has(elem)) {
            _intersection.add(elem);
        }
    }
    return _intersection;
}

function hasIntersectionSets(setA, setB){
    var _intersection = new Set();
    for (var elem of setB) {
        if (setA.has(elem)) {
            return true;
        }
    }
    return false;
}